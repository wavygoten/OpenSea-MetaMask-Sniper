{
	"name": "Sniper Tool",
	"description": "Snipe items on Opensea",
	"manifest_version": 2,
	"version": "1.0.0",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "icon16.png",
			"48": "icon48.png"
		},
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"content_scripts": [
		{
			"matches": ["https://opensea.io/*"],
			"js": ["content.js"]
		}
	]
}
